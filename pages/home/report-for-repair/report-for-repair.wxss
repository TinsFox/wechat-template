.tui-header{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20rpx;
    box-sizing: border-box;
    background-color: #fff;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    overflow: hidden;
    position: relative;
  }
  .tui-icon-close{
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%);
  }